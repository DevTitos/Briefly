{
	"name": "adk-hono-server",
	"version": "0.0.1",
	"description": "ADK Hono Server - A starter project for creating AI agents with Hono web framework",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"build": "tsc",
		"dev": "tsx watch src/index.ts",
		"start": "node dist/index.js",
		"clean": "rimraf dist",
		"prebuild": "pnpm clean",
		"db:setup": "tsx src/scripts/setup-db.ts",
		"db:generate": "drizzle-kit generate:sqlite",
		"db:push": "drizzle-kit push"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/IQAICOM/adk-hono-server.git"
	},
	"keywords": [
		"ai",
		"agent",
		"mcp",
		"adk-ts",
		"IQAI",
		"hono",
		"web-server"
	],
	"author": "IQAI",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/IQAICOM/adk-hono-server/issues"
	},
	"homepage": "https://github.com/IQAICOM/adk-hono-server#readme",
	"dependencies": {
		"@hono/node-server": "^1.19.2",
		"@iqai/adk": "^0.5.0",
		"@libsql/client": "^0.15.15",
		"bcryptjs": "^3.0.2",
		"dotenv": "^17.2.2",
		"drizzle-orm": "^0.44.6",
		"hono": "^4.9.6",
		"openai": "^6.6.0",
		"uuid": "^13.0.0",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/node": "^24.3.1",
		"@types/uuid": "^10.0.0",
		"drizzle-kit": "^0.31.5",
		"rimraf": "^6.0.1",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2"
	}
}
